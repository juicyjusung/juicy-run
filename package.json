{
  "author": "juicyjusung<juicyjusung@gmail.com>",
  "bin": {
    "juicy-run": "./bin/run.js",
    "jr": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-not-found": "^3.0.9",
    "@oclif/plugin-plugins": "^4",
    "conf": "^12.0.0",
    "inquirer": "^9.2.12",
    "inquirer-search-list": "^1.2.6"
  },
  "description": "Easily manage and execute package.json scripts with your choice of package manager.",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.4.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/juicyjusung/juicy-run",
  "license": "MIT",
  "main": "",
  "name": "juicy-run",
  "oclif": {
    "default": ".",
    "commands": "./dist",
    "bin": "jr",
    "dirname": "jr",
    "plugins": [
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "juicyjusung/juicy-run",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "1.0.1",
  "bugs": "https://github.com/juicyjusung/juicy-run/issues",
  "keywords": [
    "juicy-run"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
